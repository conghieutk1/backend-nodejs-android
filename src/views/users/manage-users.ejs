        <script>
            let users = {};
            document.addEventListener('DOMContentLoaded', async (event) => {
                let response = await axios.get('/get-all-user?pageNumber=1');
                ;
                users = response.data.users;
                console.log('user = ', response.data.users);
                const table = document.getElementById('user-table');
                if (users.length == 0) {
                    table.innerHTML = '<td colspan="8" class="text-center">Không có dữ liệu</td>'
                }
                console.log('users = ', users[0]);
                users.map((data, index) => {
                    const tbRow = document.createElement('tr');
                    tbRow.innerHTML = `
                        <td >${index + 1}</td>
                        <td >${data.account}</td>
                        <td >${data.fullName}</td>
                        <td >${data.gender}</td>
                        <td >${data.phoneNumber}</td>
                         <td >${data.address}</td>
                        <td>
                            <div class="d-flex align-items-center list-action">
                                <a class="badge bg-success mr-2" data-toggle="tooltip" data-placement="top" title="" data-original-title="Edit"
                                    href="/edit-crud/?id=${data.id}"><i class="ri-pencil-line mr-0"></i></a>
                                <a class="badge bg-warning mr-2" data-toggle="tooltip" data-placement="top" title="" data-original-title="Delete"
                                    href="/delete-crud/?id=${data.id}"><i class="ri-delete-bin-line mr-0"></i></a>
                            </div>
                        </td>
                    
                    `
                    
                    table.appendChild(tbRow);
                })
            });

            // Đảm bảo rằng biến 'message' đã được truyền từ phía máy chủ
            var message = "<%= message %>";

            // Kiểm tra xem có thông báo nào được trả về không
            if (message) {
                Toastify({
                    text: message,
                    duration: 3000,
                    destination: "https://github.com/apvarun/toastify-js",
                    newWindow: true,
                    close: true,
                    gravity: "bottom", // `top` or `bottom`
                    position: "right", // `left`, `center` or `right`
                    stopOnFocus: true, // Prevents dismissing of toast on hover
                    style: {
                        background: "linear-gradient(to right, #00b09b, #96c93d)",
                    },
                onClick: function(){} // Callback after click
                }).showToast();
            }
        </script>
