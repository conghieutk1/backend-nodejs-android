        <script>
            document.addEventListener('DOMContentLoaded', (event) => {
                // Lấy link ảnh từ localStorage
                const userInfoStorage = localStorage.getItem('userInfo');

                // Kiểm tra xem link ảnh có tồn tại không
                if (userInfoStorage) {
                    // Gán link ảnh vào thẻ <img>
                    const userInfo = JSON.parse(userInfoStorage);
                    // const profileImg = document.getElementById('profile-img');
                    const profileImages = document.querySelectorAll('.profile-img');
                    const profileName = document.getElementById('profile-name');
                    const textWelcome = document.getElementById('text-welcome');
                    const timeLogin = document.getElementById('time-expiration');

                    profileImages.forEach((img) => {
                        img.src = userInfo.user.photoURL;
                    });
                    // profileImg.src = userInfo.user.photoURL;
                    profileName.innerHTML = userInfo.user.email;
                    textWelcome.innerHTML = `Xin chào, ${userInfo.user.displayName}`;
                    timeLogin.innerHTML = `${userInfo.user.expirationTime}`;
                }
            });
        </script>
        <script>
            async function getDataForChart() {
                let dataFromServer = [];
                try {
                    let response = await axios.get('/get-data-for-predict-chart?time=week');
                    dataFromServer = response.data;
                } catch (error) {
                    console.error('Error fetching data:', error);
                }
                return dataFromServer;
            }
            async function init() {
                let dataFromServer = await getDataForChart();
                console.log('dataFromServer = ', dataFromServer);
                // document.getElementById('greeting').innerText = 'Data fetched successfully';
                const ctx = document.getElementById('diseaseChart').getContext('2d');
                const data = {
                    labels: dataFromServer.diseases,
                    datasets: [
                        {
                            label: 'Tổng quan về chẩn đoán bệnh',
                            data: dataFromServer.amount,
                            backgroundColor: [
                                'rgb(255, 99, 132)',
                                'rgb(54, 162, 235)',
                                'rgb(255, 205, 86)',
                                'rgb(75, 192, 192)',
                                'rgb(153, 102, 255)',
                                'rgb(255, 159, 64)',
                                'rgb(201, 203, 207)',
                            ],
                            borderWidth: 1,
                            borderColor: 'rgba(255, 255, 255, 1)', // Sử dụng màu trắng cho viền
                        },
                    ],
                };

                const config = {
                    type: 'pie',
                    data: data,
                    options: {
                        responsive: true,
                        plugins: {
                            legend: {
                                position: 'top',
                            },
                            title: {
                                display: false,
                                text: 'Tổng quan về chẩn đoán bệnh',
                            },
                        },
                    },
                };

                const diseaseChart = new Chart(ctx, config);
            }
            init();
        </script>
